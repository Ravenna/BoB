

<div id="page_body">
	<%=m @page.body %>
	
<hr> 

<h2 class="pad_top">Award Categories</h2>

<h4>BRAND AWARDS (OPEN TO ALL ASSOCIATES)</h4>



	<ul class="award_cat_list">

	<% @corp.each do |award| %>
	
		<li><%= link_to award.name, category_award_path(award, :category_id => @corpcat ) %></li>
	<% end %>
	</ul>
	
	

<h4>STORE STATISTICAL AWARDS (OPEN TO STORES DIVISION)</h4>
	<ul class="award_cat_list">		
		<% @store.each do |award| %>
			<li><%= link_to award.name, category_award_path(award, :category_id => @storecat) %></li>
		<% end %>
	</ul>
	<h4>SENIOR MANAGEMENT TEAM SELECTED AWARDS</h4>
		<ul class="award_cat_list">
			<li><%= link_to "Best Product Wins", category_award_path(@cat4, @award15) %></li>
			<li><%= link_to "Emerging Leader", category_award_path(@cat4, @award24) %></li>
		</ul>


</div>